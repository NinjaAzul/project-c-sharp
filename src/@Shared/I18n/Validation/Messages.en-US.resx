  </data>
  <data name="Password_RequireUpperCase" xml:space="preserve">
    <value>The password must contain at least one uppercase letter</value>
  </data>
  <data name="Password_RequireNumber" xml:space="preserve">
    <value>The password must contain at least one number</value>
  </data>
  <data name="Required_Field" xml:space="preserve">
    <value>The {0} field is required</value>
  </data>
  <data name="Invalid_Email" xml:space="preserve">
    <value>The provided email is not valid</value>
  </data>
  <data name="Length_Between" xml:space="preserve">
    <value>The {0} field must be between {1} and {2} characters</value>
  </data>
</root>